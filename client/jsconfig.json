{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "nodenext",
    "paths": {
      "@scss/*": ["src/scss/*"],
      "@utils/*": ["src/utils/*"],
      "@features/*": ["src/features/*"],
      "@components/*": ["src/components/*"],
      "@actions/*": ["src/actions/*"],
      "@constants/*": ["src/constants/*"],
      "@Services/*": ["src/services/*"],
      "@Contexts/*": ["src/contexts/*"],
      "@CustomHooks/*": ["src/customHooks/*"]
    }
  },
  "exclude": ["node_modules"]
}
